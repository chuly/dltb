	<script>
		var urlparser = new URLParser();

		var strCODE = urlparser.getQueryData("code");  	

		function getRandom(n) 
		{
			return Math.floor(Math.random() * n + 1)
		}		
		function bytesToSize(bytes) 
		{  
			if (bytes === 0) return '0 B';  
			
			var k = 1024;  
			
			sizes = ['B','KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'];  
			
			i = Math.floor(Math.log(bytes) / Math.log(k));  
		  
			//return (bytes / Math.pow(k, i)) + ' ' + sizes[i];   
			//toPrecision(3) 后面保留一位小数，如1.0GB
			return (bytes / Math.pow(k, i)).toPrecision(3) + ' ' + sizes[i];  
		}  
		
		function onFanyi(result)
		{
			console.log(result.ret);
			var nRet = result.ret;
			if (nRet != 0)
			{
				
				return ;
			}			
			var data = result.data;
			console.log(data);
			$("#p_fanyi").text(decodeURIComponent(atob(data.substring(7,data.length).split("").reverse().join(""))));
			
		}
		
		function fanyi(word)
		{
			$.ajax({
				type: "GET",
				url: "http://api2.ciliwan.com:84/magnet.py/get_fanyi_by_word",
				success: onFanyi,
				dataType: "jsonp",
				data: {
					word:"",
					base64word:"CiLiWan"+btoa(encodeURIComponent(word)),
					r: getRandom(9999)
				},
				timeout: 8000
			});					
		}

		function onGetBTIH(data)
		{
			console.log(data.ret);
			var nRet = data.ret;
			if (nRet != 0)
			{
				
				return ;
			}			
			var info = data.info;
			$("#p_magnet").text(decodeURIComponent(info.title));
			$("#span_size").text(bytesToSize(info.size));
			$("#span_count").text(info.count);			
			
			$("#h2_btih").text(info.btih);
			$("#a_magnet").attr("href", info.btih);
			$("#a_baidu").attr("href","https://www.baidu.com/s?wd="+info.title);			
			$("#b_copy").attr("data-clipboard-text", info.btih);
			document.title = "CiLiWan.COM - "+decodeURIComponent(info.title);
			SetClipBoard(info.btih);
			
			$("#a_fanyi").attr("href","javascript:fanyi('"+info.title+"')");			
		
		}
		
		function SetClipBoard(btih)
		{
			var clipboard = new Clipboard('.copybtn');
		
			var iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;
		
			clipboard.on('success', function (e) {
				alert("复制成功");
		
				e.clearSelection();
			});
		
			clipboard.on('error', function (e) {
				if (iOS) {
					alert("苹果设备不支持浏览器复制");
				}
				else 
				{
					if (typeof(window.PSOU_JS_CLIENT)!="undefined")
						window.PSOU_JS_CLIENT.SetClipBoard(btih);
					else
						alert("浏览器不支持复制");
				}
		
			});
		

		}



		Date.prototype.Format = function(format){ 
		var o = { 
		"M+" : this.getMonth()+1, //month 
		"d+" : this.getDate(), //day 
		"h+" : this.getHours(), //hour 
		"m+" : this.getMinutes(), //minute 
		"s+" : this.getSeconds(), //second 
		"q+" : Math.floor((this.getMonth()+3)/3), //quarter 
		"S" : this.getMilliseconds() //millisecond 
		} 
		
		if(/(y+)/.test(format)) { 
		format = format.replace(RegExp.$1, (this.getFullYear()+"").substr(4 - RegExp.$1.length)); 
		} 
		
		for(var k in o) { 
		if(new RegExp("("+ k +")").test(format)) { 
		format = format.replace(RegExp.$1, RegExp.$1.length==1 ? o[k] : ("00"+ o[k]).substr((""+ o[k]).length)); 
		} 
		} 
		return format; 
		} 		
		
	
		
		function IsPC()
		{
		   var userAgentInfo = navigator.userAgent;
		   var Agents = new Array("Android", "iPhone", "SymbianOS", "Windows Phone", "iPad", "iPod");
		   var flag = true;
		   for (var v = 0; v < Agents.length; v++) {
			   if (userAgentInfo.indexOf(Agents[v]) > 0) { flag = false; break; }
		   }
		   for (var v = 0; v < Agents.length; v++) {
			   if (navigator.appVersion.indexOf(Agents[v]) > 0) { flag = false; break; }
		   }		   
		   return flag;
		}
		
